get_game_wl <- function(data) {
win_count <- 0
loss_count <- 0
wl_vec <- c()
for (i in 1:nrow(data)) {
if (data[i, 4] == "W") {
win_count = win_count + 1
}
else {
loss_count = loss_count + 1
}
if (loss_count == 0) {
wl_vec <- c(wl_vec, NA)
}
else {
wl_vec <- c(wl_vec, win_count / loss_count)
}
}
wl_vec
}
